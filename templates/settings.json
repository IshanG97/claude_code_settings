{
  "includeCoAuthoredBy": false,
  "permissions": {
    "allowFileAccess": true,
    "allowNetworkAccess": true,
    "allowShellAccess": true,
    "allow": [
	"Bash(command:*)",
	"Bash(git diff:*)",
	"WebSearch",
	"Read(//Applications/**)",
	"Bash(brew list:*)",
	"Read(//usr/local/bin/**)"
    ]
  },
  "preferences": {
    "autoSave": true,
    "showLineNumbers": true,
    "tabSize": 2,
    "insertSpaces": true,
    "wordWrap": true
  }
}
